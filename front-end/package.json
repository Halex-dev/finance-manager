{
  "name": "vuestic-admin",
  "private": true,
  "version": "3.1.0",
  "scripts": {
    "prepare": "husky install",
    "dev": "vite",
    "dev:host": "vite --host",
    "build": "npm run lint && vue-tsc --noEmit && vite build",
    "build-e": "npm run lint && vue-tsc --noEmit && vite build",
    "build:ci": "vite build",
    "start:ci": "serve -s ./dist",
    "prelint": "npm run format",
    "lint": "eslint \"./src/**/*.{ts,js,vue}\" --fix",
    "format": "prettier --write .",
    "preview": "vite preview",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "lint-staged": {
    "./src/**/*.{ts,js,vue}": [
      "npm run lint"
    ]
  },
  "dependencies": {
    "@gtm-support/vue-gtm": "^2.2.0",
    "@vuestic/tailwind": "^0.1.5",
    "@vueuse/core": "^10.9.0",
    "axios": "^1.6.8",
    "chart.js": "^4.4.2",
    "chartjs-chart-geo": "^4.3.0",
    "date-fns": "^3.6.0",
    "epic-spinners": "^2.0.0",
    "flag-icons": "^7.2.1",
    "ionicons": "^7.4.0",
    "medium-editor": "^5.23.3",
    "pinia": "^2.1.7",
    "register-service-worker": "^1.7.2",
    "sass": "^1.76.0",
    "serve": "^14.2.3",
    "tinycolor2": "^1.6.0",
    "vue": "^3.4.21",
    "vue-chartjs": "^5.3.1",
    "vue-i18n": "^9.13.1",
    "vue-router": "^4.3.2",
    "vuestic-ui": "^1.9.9"
  },
  "devDependencies": {
    "@intlify/unplugin-vue-i18n": "^4.0.0",
    "@types/medium-editor": "^5.0.8",
    "@types/node": "^20.12.8",
    "@types/tinycolor2": "^1.4.6",
    "@typescript-eslint/eslint-plugin": "^7.8.0",
    "@typescript-eslint/parser": "^7.8.0",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^13.0.0",
    "autoprefixer": "^10.4.19",
    "eslint": "^8.57.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-storybook": "^0.8.0",
    "eslint-plugin-vue": "^9.25.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "postcss": "^8.4.38",
    "prettier": "^3.2.5",
    "tailwindcss": "^3.4.3",
    "typescript": "^5.4.5",
    "vite": "^5.2.11",
    "vue-eslint-parser": "^9.4.2",
    "vue-tsc": "^2.0.16"
  }
}
